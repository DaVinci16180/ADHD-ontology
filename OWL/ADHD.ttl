@prefix : <http://adhd.com#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix gufo: <http://purl.org/nemo/gufo#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://adhd.com#> .

<http://adhd.com> rdf:type owl:Ontology .

#################################################################
#    Object Properties
#################################################################

###  http://adhd.com#hasBirthSex
:hasBirthSex rdf:type owl:ObjectProperty ,
                      owl:FunctionalProperty ;
             rdfs:domain :Patient ;
             rdfs:range :BirthSex .


###  http://adhd.com#hasSeverity
:hasSeverity rdf:type owl:ObjectProperty ,
                      owl:FunctionalProperty ;
             rdfs:range :Severity .


###  http://adhd.com#isGreaterThan
:isGreaterThan rdf:type owl:ObjectProperty ;
               owl:inverseOf :isLessThan ;
               rdf:type owl:TransitiveProperty ;
               rdfs:domain :Severity ;
               rdfs:range :Severity .


###  http://adhd.com#isLessThan
:isLessThan rdf:type owl:ObjectProperty ,
                     owl:TransitiveProperty ;
            rdfs:domain :Severity ;
            rdfs:range :Severity .


###  http://adhd.com#prescribes
:prescribes rdf:type owl:ObjectProperty ;
            rdfs:domain :Prescription ;
            rdfs:range :MedicationTherapy .


###  http://adhd.com#suffersFrom
:suffersFrom rdf:type owl:ObjectProperty ;
             rdfs:domain :Patient ;
             rdfs:range :MedicalCondition .


###  http://purl.org/nemo/gufo#externallyDependsOn
gufo:externallyDependsOn rdf:type owl:ObjectProperty .


###  http://purl.org/nemo/gufo#inheresIn
gufo:inheresIn rdf:type owl:ObjectProperty .


###  http://purl.org/nemo/gufo#mediates
gufo:mediates rdf:type owl:ObjectProperty .


#################################################################
#    Data properties
#################################################################

###  http://adhd.com#hasDateOfBirth
:hasDateOfBirth rdf:type owl:DatatypeProperty ,
                         owl:FunctionalProperty ;
                rdfs:domain :Person ;
                rdfs:range xsd:dateTime .


###  http://adhd.com#hasDocument
:hasDocument rdf:type owl:DatatypeProperty ;
             rdfs:domain :Patient ;
             rdfs:range xsd:string .


###  http://adhd.com#hasEmissionDate
:hasEmissionDate rdf:type owl:DatatypeProperty ,
                          owl:FunctionalProperty ;
                 rdfs:domain :MedicalReport ;
                 rdfs:range xsd:dateTime .


###  http://adhd.com#hasName
:hasName rdf:type owl:DatatypeProperty ;
         rdfs:domain :Person ;
         rdfs:range xsd:string .


###  http://adhd.com#hasValue
:hasValue rdf:type owl:DatatypeProperty ,
                   owl:FunctionalProperty ;
          rdfs:domain :Severity ;
          rdfs:range xsd:integer .


###  http://adhd.com#isSatisfied
:isSatisfied rdf:type owl:DatatypeProperty ,
                      owl:FunctionalProperty ;
             rdfs:domain [ rdf:type owl:Class ;
                           owl:unionOf ( :CriterionA
                                         :CriterionB
                                         :CriterionC
                                         :CriterionD
                                         :CriterionE
                                       )
                         ] ;
             rdfs:range xsd:boolean .


#################################################################
#    Classes
#################################################################

###  http://adhd.com#ADHD
:ADHD rdf:type owl:Class ;
      owl:equivalentClass [ owl:intersectionOf ( :NeurologicallyBasedCondition
                                                 [ rdf:type owl:Class ;
                                                   owl:unionOf ( [ rdf:type owl:Restriction ;
                                                                   owl:onProperty [ owl:inverseOf gufo:inheresIn
                                                                                  ] ;
                                                                   owl:someValuesFrom :CriterionA1
                                                                 ]
                                                                 [ rdf:type owl:Restriction ;
                                                                   owl:onProperty [ owl:inverseOf gufo:inheresIn
                                                                                  ] ;
                                                                   owl:someValuesFrom :CriterionA2
                                                                 ]
                                                               )
                                                 ]
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty [ owl:inverseOf gufo:inheresIn
                                                                  ] ;
                                                   owl:someValuesFrom :CriterionB
                                                 ]
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty [ owl:inverseOf gufo:inheresIn
                                                                  ] ;
                                                   owl:someValuesFrom :CriterionC
                                                 ]
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty [ owl:inverseOf gufo:inheresIn
                                                                  ] ;
                                                   owl:someValuesFrom :CriterionD
                                                 ]
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty [ owl:inverseOf gufo:inheresIn
                                                                  ] ;
                                                   owl:someValuesFrom :CriterionE
                                                 ]
                                               ) ;
                            rdf:type owl:Class
                          ] .


###  http://adhd.com#ADHDCriteria
:ADHDCriteria rdf:type owl:Class ;
              owl:equivalentClass [ rdf:type owl:Class ;
                                    owl:unionOf ( :CriterionA
                                                  :CriterionB
                                                  :CriterionC
                                                  :CriterionD
                                                  :CriterionE
                                                )
                                  ] ;
              rdfs:subClassOf gufo:FunctionalComplex ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty gufo:inheresIn ;
                                owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onClass :ADHD
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :isSatisfied ;
                                owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onDataRange xsd:boolean
                              ] .


###  http://adhd.com#Adult
:Adult rdf:type owl:Class ;
       rdfs:subClassOf :Patient .


###  http://adhd.com#BehavioralTherapy
:BehavioralTherapy rdf:type owl:Class ;
                   rdfs:subClassOf gufo:IntrinsicMode ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty gufo:externallyDependsOn ;
                                     owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                     owl:onClass :Psychologist
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty gufo:inheresIn ;
                                     owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                     owl:onClass :Patient
                                   ] .


###  http://adhd.com#BirthSex
:BirthSex rdf:type owl:Class ;
          owl:equivalentClass [ rdf:type owl:Class ;
                                owl:oneOf ( :Female
                                            :Male
                                          )
                              ] ;
          rdfs:subClassOf gufo:Quality .


###  http://adhd.com#CriterionA
:CriterionA rdf:type owl:Class ;
            rdfs:subClassOf :ADHDCriteria ;
            owl:disjointWith :CriterionB ,
                             :CriterionC ,
                             :CriterionD ,
                             :CriterionE .


###  http://adhd.com#CriterionA1
:CriterionA1 rdf:type owl:Class ;
             owl:equivalentClass [ owl:intersectionOf ( :CriterionA
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty [ owl:inverseOf gufo:inheresIn
                                                                         ] ;
                                                          owl:minQualifiedCardinality "6"^^xsd:nonNegativeInteger ;
                                                          owl:onClass :HyperactivitySymptom
                                                        ]
                                                      ) ;
                                   rdf:type owl:Class
                                 ] .


###  http://adhd.com#CriterionA1a
:CriterionA1a rdf:type owl:Class ;
              rdfs:subClassOf :HyperactivitySymptom .


