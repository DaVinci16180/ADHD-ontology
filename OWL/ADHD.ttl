#################################################################
#    Object Properties
#################################################################

###  http://example.com#hasSymptom
:hasSymptom rdf:type owl:ObjectProperty ;
            rdfs:domain :MedicalCondition ;
            rdfs:range :Symptom .


###  http://example.com#prescribes
:prescribes rdf:type owl:ObjectProperty ;
            rdfs:domain :Prescription ;
            rdfs:range :MedicationTherapy .


###  http://example.com#suffersFrom
:suffersFrom rdf:type owl:ObjectProperty ;
             rdfs:domain :Patient ;
             rdfs:range :MedicalCondition .


###  http://purl.org/nemo/gufo#externallyDependsOn
gufo:externallyDependsOn rdf:type owl:ObjectProperty .


###  http://purl.org/nemo/gufo#inheresIn
gufo:inheresIn rdf:type owl:ObjectProperty .


###  http://purl.org/nemo/gufo#mediates
gufo:mediates rdf:type owl:ObjectProperty .


#################################################################
#    Data properties
#################################################################

###  http://example.com#dosage
:dosage rdf:type owl:DatatypeProperty ;
        rdfs:domain :Medicine ;
        rdfs:range xsd:string .


###  http://example.com#hasDateOfBirth
:hasDateOfBirth rdf:type owl:DatatypeProperty ;
                rdfs:domain :Person ;
                rdfs:range xsd:dateTime .


###  http://example.com#hasDocument
:hasDocument rdf:type owl:DatatypeProperty ;
             rdfs:domain :Patient ;
             rdfs:range xsd:string .


###  http://example.com#hasName
:hasName rdf:type owl:DatatypeProperty ;
         rdfs:domain :Person ;
         rdfs:range xsd:string .


#################################################################
#    Classes
#################################################################

###  http://example.com#ADHD
:ADHD rdf:type owl:Class ;
      owl:equivalentClass [ rdf:type owl:Class ;
                            owl:unionOf ( :Combined
                                          :PredominantlyHyperactive
                                          :PredominantlyInattentive
                                        )
                          ] ;
      rdfs:subClassOf :NeurologicallyBasedCondition .


###  http://example.com#Adult
:Adult rdf:type owl:Class ;
       rdfs:subClassOf :Patient .


###  http://example.com#BehavioralTherapy
:BehavioralTherapy rdf:type owl:Class ;
                   rdfs:subClassOf gufo:IntrinsicMode ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty gufo:externallyDependsOn ;
                                     owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                     owl:onClass :Psychologist
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty gufo:inheresIn ;
                                     owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                     owl:onClass :Patient
                                   ] .


###  http://example.com#Combined
:Combined rdf:type owl:Class ;
          owl:equivalentClass [ owl:intersectionOf ( :ADHD
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty :hasSymptom ;
                                                       owl:minQualifiedCardinality "6"^^xsd:nonNegativeInteger ;
                                                       owl:onClass :HyperactivitySymptom
                                                     ]
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty :hasSymptom ;
                                                       owl:minQualifiedCardinality "6"^^xsd:nonNegativeInteger ;
                                                       owl:onClass :InattentionSymptom
                                                     ]
                                                   ) ;
                                rdf:type owl:Class
                              ] .


###  http://example.com#Dextroamphetamine
:Dextroamphetamine rdf:type owl:Class ;
                   rdfs:subClassOf :Medicine ;
                   owl:disjointWith :Methylphenidate .


###  http://example.com#DifficultyAwaitingTurn
:DifficultyAwaitingTurn rdf:type owl:Class ;
                        owl:equivalentClass [ rdf:type owl:Class ;
