          rdfs:subClassOf gufo:FunctionalComplex .


###  http://adhd.com#Methylphenidate
:Methylphenidate rdf:type owl:Class ;
                 rdfs:subClassOf :Medicine .


###  http://adhd.com#NeurologicallyBasedCondition
:NeurologicallyBasedCondition rdf:type owl:Class ;
                              rdfs:subClassOf :MedicalCondition .


###  http://adhd.com#Patient
:Patient rdf:type owl:Class ;
         rdfs:subClassOf :Person ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :suffersFrom ;
                           owl:someValuesFrom :MedicalCondition
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :hasBirthSex ;
                           owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                           owl:onClass :BirthSex
                         ] .


###  http://adhd.com#Person
:Person rdf:type owl:Class ;
        rdfs:subClassOf gufo:FunctionalComplex ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :hasName ;
                          owl:someValuesFrom xsd:string
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :hasDateOfBirth ;
                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:onDataRange xsd:dateTime
                        ] .


###  http://adhd.com#PreschoolAge
:PreschoolAge rdf:type owl:Class ;
              rdfs:subClassOf :Patient .


###  http://adhd.com#Prescription
:Prescription rdf:type owl:Class ;
              rdfs:subClassOf gufo:Relator ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :prescribes ;
                                owl:someValuesFrom :MedicationTherapy
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty gufo:mediates ;
                                owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onClass :Doctor
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty gufo:mediates ;
                                owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onClass :Patient
                              ] .


###  http://adhd.com#Psychologist
:Psychologist rdf:type owl:Class ;
              rdfs:subClassOf :Person .


###  http://adhd.com#SchoolAge
:SchoolAge rdf:type owl:Class ;
           rdfs:subClassOf :Patient .


###  http://adhd.com#Severity
:Severity rdf:type owl:Class ;
          owl:equivalentClass [ rdf:type owl:Class ;
                                owl:oneOf ( :Severity1
                                            :Severity2
                                            :Severity3
                                            :Severity4
