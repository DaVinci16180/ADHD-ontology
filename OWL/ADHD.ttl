###  http://adhd.com#Dextroamphetamine
:Dextroamphetamine rdf:type owl:Class ;
                   rdfs:subClassOf :Medicine ;
                   owl:disjointWith :Methylphenidate .


###  http://adhd.com#Doctor
:Doctor rdf:type owl:Class ;
        rdfs:subClassOf :Person .


###  http://adhd.com#HyperactivitySymptom
:HyperactivitySymptom rdf:type owl:Class ;
                      owl:equivalentClass [ owl:intersectionOf ( gufo:FunctionalComplex
                                                                 [ rdf:type owl:Restriction ;
                                                                   owl:onProperty :hasSeverity ;
                                                                   owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                   owl:onClass :Severity
                                                                 ]
                                                                 [ rdf:type owl:Restriction ;
                                                                   owl:onProperty gufo:inheresIn ;
                                                                   owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                   owl:onClass :CriterionA1
                                                                 ]
                                                               ) ;
                                            rdf:type owl:Class
                                          ] ;
                      owl:disjointWith :InattentionSymptom .


###  http://adhd.com#InattentionSymptom
:InattentionSymptom rdf:type owl:Class ;
                    owl:equivalentClass [ owl:intersectionOf ( gufo:FunctionalComplex
                                                               [ rdf:type owl:Restriction ;
                                                                 owl:onProperty :hasSeverity ;
                                                                 owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                 owl:onClass :Severity
                                                               ]
                                                               [ rdf:type owl:Restriction ;
                                                                 owl:onProperty gufo:inheresIn ;
                                                                 owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                 owl:onClass :CriterionA2
                                                               ]
                                                             ) ;
                                          rdf:type owl:Class
                                        ] .


###  http://adhd.com#MedicalCondition
:MedicalCondition rdf:type owl:Class ;
                  rdfs:subClassOf gufo:FunctionalComplex .


###  http://adhd.com#MedicalReport
:MedicalReport rdf:type owl:Class ;
               rdfs:subClassOf gufo:Relator ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty gufo:mediates ;
                                 owl:someValuesFrom :Doctor
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty gufo:mediates ;
                                 owl:someValuesFrom :MedicalCondition
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty gufo:mediates ;
                                 owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                 owl:onClass :Patient
                               ] .


###  http://adhd.com#MedicationTherapy
:MedicationTherapy rdf:type owl:Class ;
                   rdfs:subClassOf gufo:IntrinsicMode ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty gufo:externallyDependsOn ;
                                     owl:someValuesFrom :Medicine
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty gufo:inheresIn ;
                                     owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                     owl:onClass :Patient
                                   ] .


###  http://adhd.com#Medicine
